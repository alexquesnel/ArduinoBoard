void setup() {
  pinMode(13, OUTPUT);

}

void loop() {
  digitalWrite(13, HIGH);
  delay(1000);
  digitalWrite(13, LOW);
  delay(1000);

}



tests with pins

void setup() {
  pinMode(2, OUTPUT);
  pinMode(4, INPUT);

}

void loop() {
  if(digitalRead(4)){
    digitalWrite(2, HIGH);
  }else{
    digitalWrite(2, LOW);
  }

}


